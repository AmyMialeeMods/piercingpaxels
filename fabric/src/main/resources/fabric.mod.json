{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_author}"
    ],
    "contributors": [
        "${mod_credits}"
    ],
    "contact": {
        "homepage": "https://amymialee.xyz/",
        "sources": "https://github.com/AmyMialeeMods/piercingpaxels",
        "issues": "https://github.com/AmyMialeeMods/piercingpaxels/issues"
    },
    "license": "${mod_license}",
    "icon": "assets/piercingpaxels/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "xyz.amymialee.piercingpaxels.PiercingPaxelsFabric"
        ],
        "client": [
            "xyz.amymialee.piercingpaxels.PiercingPaxelsFabricClient"
        ],
        "fabric-datagen": [
            "xyz.amymialee.piercingpaxels.PiercingPaxelsFabricDatagen"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
